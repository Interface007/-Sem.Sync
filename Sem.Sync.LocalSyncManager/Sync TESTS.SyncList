<?xml version="1.0"?>
<ArrayOfSyncDescription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <SyncDescription Name="Delete previous export/work files">
    <Command>DeletePattern</Command>
    <TargetStorePath>
      {FS:WorkingFolder}\Outlook.xmlcontact
      {FS:WorkingFolder}\wkw.xmlcontact
    </TargetStorePath>
  </SyncDescription>

  <SyncDescription Name="Export from Outlook to file system">
    <Command>CopyAll</Command>
    <SourceConnector>Sem.Sync.OutlookConnector.ContactClient</SourceConnector>
    <TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
    <TargetStorePath>{FS:WorkingFolder}\Outlook.xmlcontact</TargetStorePath>
  </SyncDescription>

  <SyncDescription Name="Read from Wer-Kennt-Wen to file system">
		<Command>CopyAll</Command>
		<SourceConnector>Sem.Sync.WerKenntWenConnector.ContactClient</SourceConnector>
		<TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
		<TargetStorePath>{FS:WorkingFolder}\wkw.xmlcontact</TargetStorePath>
	</SyncDescription>

  <SyncDescription Name="Match entries manually">
    <Command>MatchManually</Command>
    <CommandParameter>WerKenntWenUrl</CommandParameter>
    <SourceConnector>Sem.Sync.FilesystemConnector.ContactClient</SourceConnector>
    <SourceStorePath>{FS:WorkingFolder}\wkw.xmlcontact</SourceStorePath>
    <TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
    <TargetStorePath>{FS:WorkingFolder}\Outlook.xmlcontact</TargetStorePath>
    <BaselineConnector>Sem.Sync.FilesystemConnector.GenericClient`1 of Sem.Sync.SyncBase.DetailData.MatchingEntry, Sem.Sync.SyncBase</BaselineConnector>
    <BaselineStorePath>{FS:WorkingFolder}\BaseLine.xmlbase</BaselineStorePath>
  </SyncDescription>

  <SyncDescription Name="Match wkw by Profile Id to Baseline">
    <Command>MatchByProfileId</Command>
    <SourceConnector>Sem.Sync.FilesystemConnector.ContactClient</SourceConnector>
    <SourceStorePath>{FS:WorkingFolder}\wkw.xmlcontact</SourceStorePath>
    <BaselineConnector>Sem.Sync.FilesystemConnector.GenericClient`1 of Sem.Sync.SyncBase.DetailData.MatchingEntry, Sem.Sync.SyncBase</BaselineConnector>
    <BaselineStorePath>{FS:WorkingFolder}\BaseLine.xmlbase</BaselineStorePath>
    <TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
    <TargetStorePath>{FS:WorkingFolder}\wkw.xmlcontact</TargetStorePath>
  </SyncDescription>

  <SyncDescription Name="Add missing from Outlook to wkw (file system)">
    <Command>MergeMissing</Command>
    <SourceConnector>Sem.Sync.FilesystemConnector.ContactClient</SourceConnector>
    <SourceStorePath>{FS:WorkingFolder}\Outlook.xmlcontact</SourceStorePath>
    <TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
    <TargetStorePath>{FS:WorkingFolder}\wkw.xmlcontact</TargetStorePath>
  </SyncDescription>

  <SyncDescription Name="Add missing from wkw to Outlook (file system)">
    <Command>MergeMissing</Command>
    <SourceConnector>Sem.Sync.FilesystemConnector.ContactClient</SourceConnector>
    <SourceStorePath>{FS:WorkingFolder}\wkw.xmlcontact</SourceStorePath>
    <TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
    <TargetStorePath>{FS:WorkingFolder}\Outlook.xmlcontact</TargetStorePath>
  </SyncDescription>

  <SyncDescription Name="Merge high evidence from wkw to Outlook (file system)">
    <Command>MergeHighEvidence</Command>
    <SourceConnector>Sem.Sync.FilesystemConnector.ContactClient</SourceConnector>
    <SourceStorePath>{FS:WorkingFolder}\wkw.xmlcontact</SourceStorePath>
    <TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
    <TargetStorePath>{FS:WorkingFolder}\Outlook.xmlcontact</TargetStorePath>
  </SyncDescription>

  <SyncDescription Name="Merge high evidence from Outlook to wkw (file system)">
    <Command>MergeHighEvidence</Command>
    <SourceConnector>Sem.Sync.FilesystemConnector.ContactClient</SourceConnector>
    <SourceStorePath>{FS:WorkingFolder}\Outlook.xmlcontact</SourceStorePath>
    <TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
    <TargetStorePath>{FS:WorkingFolder}\wkw.xmlcontact</TargetStorePath>
  </SyncDescription>

  <SyncDescription Name="Detect Conflicts">
    <Command>DetectConflicts</Command>
    <CommandParameter>{FS:WorkingFolder}\MergeConflicts.xml</CommandParameter>
    <SourceConnector>Sem.Sync.FilesystemConnector.ContactClient</SourceConnector>
    <SourceStorePath>{FS:WorkingFolder}\wkw.xmlcontact</SourceStorePath>
    <TargetConnector>Sem.Sync.FilesystemConnector.ContactClient</TargetConnector>
    <TargetStorePath>{FS:WorkingFolder}\Outlook.xmlcontact</TargetStorePath>
  </SyncDescription>

  <SyncDescription Name="Ask if we want to overwrite Outlook">
    <Command>AskForContinue</Command>
    <CommandParameter>Do you want to import the merged contact list into your Outlook account?</CommandParameter>
    <TargetConnector>Sem.Sync.OutlookConnector.ContactClient</TargetConnector>
  </SyncDescription>

  <SyncDescription Name="Import all from file system to Outlook">
    <Command>CopyAll</Command>
    <SourceConnector>Sem.Sync.FilesystemConnector.ContactClient</SourceConnector>
    <SourceStorePath>{FS:WorkingFolder}\Outlook.xmlcontact</SourceStorePath>
    <TargetConnector>Sem.Sync.OutlookConnector.ContactClient</TargetConnector>
  </SyncDescription>

  <SyncDescription Name="Remove duplicate calendar entries">
    <Command>RemoveDuplicatesOnTarget</Command>
    <TargetConnector>Sem.Sync.OutlookConnector.CalendarClient</TargetConnector>
  </SyncDescription>

</ArrayOfSyncDescription>